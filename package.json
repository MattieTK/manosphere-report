{
  "name": "manosphere-report",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && tsc --noEmit",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply manosphere-report-db --local",
    "db:migrate:remote": "wrangler d1 migrations apply manosphere-report-db --remote"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.159.10",
    "@tanstack/react-router-devtools": "^1.159.10",
    "@tanstack/react-start": "^1.159.11",
    "drizzle-orm": "^0.38.0",
    "fast-xml-parser": "^5.0.0",
    "nanoid": "^5.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "clsx": "^2.1.0",
    "tailwind-merge": "^3.0.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.7",
    "@cloudflare/workers-types": "^4.20250214.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.5.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.6.0",
    "drizzle-kit": "^0.30.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.7.2",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.49.1"
  }
}
